{
  "name": "getaroom",
  "version": "1.0.0",
  "description": "Get a room",
  "author": "Goran Jovanov",
  "private": true,
  "scripts": {
    "dev-api": "cross-env NODE_ENV=development nodemon api/index.js --watch api",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start-api": "cross-env NODE_ENV=production node api/index.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test-api": "cross-env NODE_ENV=test node tests/drop-database.js && cross-env NODE_ENV=test cross-env TEST_TYPE=api ava --tap --verbose",
    "test-ui": "cross-env NODE_ENV=test node tests/drop-database.js && cross-env NODE_ENV=test cross-env TEST_TYPE=ui ava",
    "test": "cross-env NODE_ENV=test node tests/drop-database.js && cross-env NODE_ENV=test ava",
    "test-report-api": "cross-env NODE_ENV=test node tests/drop-database.js && cross-env NODE_ENV=test cross-env TEST_TYPE=api nyc ava --tap --timeout=300000",
    "test-report-ui": "cross-env NODE_ENV=test node tests/drop-database.js && cross-env NODE_ENV=test cross-env TEST_TYPE=ui nyc ava --tap --timeout=300000",
    "test-report": "cross-env NODE_ENV=test node tests/drop-database.js && cross-env NODE_ENV=test nyc ava --tap --timeout=300000"
  },
  "standard": {
    "env": [
      "browser",
      "node",
      "mocha"
    ],
    "ignore": [
      "client/lib/**/*.js"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/vuetify": "0.5.6",
    "@sendgrid/mail": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "cross-env": "^5.2.0",
    "fastify": "^2.6.0",
    "fastify-jwt": "^1.0.0",
    "fastify-mongodb": "^1.0.1",
    "fastify-plugin": "^1.6.0",
    "fastify-swagger": "^2.4.0",
    "fluent-schema": "^0.7.3",
    "handlebars": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.4",
    "mongoose-long": "^0.2.1",
    "nodemailer": "^6.3.0",
    "nuxt": "^2.8.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "uuid": "^3.3.2",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^0.2.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "ava": "^2.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "browser-env": "^3.2.6",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.9.0",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "expect": "^24.8.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "standard": "^13.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "supertest": "^4.0.2"
  }
}
